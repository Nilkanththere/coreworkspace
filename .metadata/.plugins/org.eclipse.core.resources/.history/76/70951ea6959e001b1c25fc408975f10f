package collectionframework;

import java.util.*;

public class Calculator {

	
	public void add()
	{ 
		int a=10;
		int b=20;
		int c=a+b;
	}
	public void sub()
	{
		int a=50;
		int b=10;
		int c=a-b;
	}
	public void mul()
	{
		int a=5;
		int b=2;
		int c=a*b;
	}
	public void div()
	{
		int a=50;
		int b=5;
		int c=a/b;
	}
	
	public static void main(String[] args) {
		
		List l=new ArrayList();
		
		Calculator c=new Calculator();
		        c.add();
		        c.sub();
		        c.mul();
		        c.div();
		           
		     l.add(c);
		     
		Iterator itr=l.iterator();     
		     
	while(itr.hasNext())
	{
		Calculator x=(Calculator)itr.next();
		System.out.println(x);
	}
		           
		           
		           
		           
		           
		           
	}
}
