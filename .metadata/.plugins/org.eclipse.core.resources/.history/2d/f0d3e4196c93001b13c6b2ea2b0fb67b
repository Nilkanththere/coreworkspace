package cjc;

public class A {

	
	public static void main(String[] args) {
		
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=5;j++){
				
			
			System.out.print(i);
			}
		
		System.out.println();
		}
		System.out.println("---------------------------------");
		
	for(int j=5;j>=1;j--)
	{
		for(int k=5;k>=1;k--)
		{
			System.out.print(j);
		}
		System.out.println();
	}
		
	System.out.println("**************************");
	
	
	for(int l=1;l<=5;l++)
	{
		for(int s=1;s<=5;s++)
		{
			System.out.print(s);
		}
		System.out.println();
	}
	
System.out.println("*********************");	
	
for(int p=5;p>=1;p--)
{
	for(int h=5;h>=1;h--)
	{
		System.out.print(h);
	}
	System.out.println();
}

System.out.println("--------------------");

	
	for(int d=1;d<=5;d++)
	{
		for(int f=5;f>=d;f--)
		{
			System.out.print(d);
		}
		System.out.println();
	}
	
	System.out.println("******************");
	
	for(int e=5;e>=1;e--)
	{
		for(int r=1;r<=e;r++)
		{
			System.out.print(e);
		}
		System.out.println();
	}
	
	System.out.println("**************");
	
	for(int b=5;b>=1;b--)
	{
		for(int q=1;q<=b;q++)
		{
			System.out.print(q);
		}
		System.out.println();
		
		
		
	}
	
	for(int i=1;i<=6;i++ )
	{
		for(int k=6;k>=i;k--)
		{
			System.out.print("*");
		}
		System.out.println();
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
		
		
}
}